<%= form_tag songs_path do %>
<%= label_tag :name %>
<%= text_field_tag :'song[name]', @song.name %>
<%= label_tag :artist %>
<%= text_field_tag :'song[artist_id]', @song.artist.id %>
<%= label_tag :genre %>
<%= text_field_tag :'song[genre_id]', @song.genre.id %>
<input type="submit" name="" value="Update Song">
<% end %>
